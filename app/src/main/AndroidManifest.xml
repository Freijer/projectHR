<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="freijer.app.projecthr">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Пhоект развитие"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="messages.MessageItem" />
        <activity android:name="notifications.notifications" />
        <activity android:name="profile.MyProfile">
            <intent-filter>
                <action android:name="profile.MyProfile" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="user.UserStart">
            <intent-filter>
                <action android:name="user.UserStart" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="admin.AdminStart">
            <intent-filter>
                <action android:name="admin.AdminStart" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="messages.messages">
            <intent-filter>
                <action android:name="messages.messages" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>